
proc tcl::mathfunc::llength {list} {llength $list}

proc tcl::mathfunc::lindex {list index} {lindex $list $index}

proc tcl::mathfunc::lassign {list args} {lassign $list {*}$args}

proc tcl::mathfunc::lappend {var args} {lappend $var {*}$args}

proc tcl::mathfunc::lempty {list} {lempty $list}

proc tcl::mathfunc::lrange {list first last} {lrange $list $first $last}




